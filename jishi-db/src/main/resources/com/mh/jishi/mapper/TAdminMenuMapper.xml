<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mh.jishi.mapper.TAdminMenuMapper">
    <select id="checkIsSuperAdmin" resultType="int">
        SELECT count(id) FROM `t_admin_menu` where admin_id = #{adminId} and menu_id = 0
    </select>
</mapper>
